{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --config ./eslint.config.cjs",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "export": "next build && next export",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:ci": "vitest run --coverage --reporter=json --reporter=default",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:api": "playwright test --project=\"API Tests\"",
    "test-runner": "node test-runner.js",
    "test:all": "node test-runner.js all",
    "test:components": "vitest run src/components/__tests__",
    "test:integration": "vitest run src/__tests__/integration"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.10.8",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "immer": "^10.1.1",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.511.0",
    "next": "15.3.1",
    "react": "~18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "~18.2.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "react-modal": "^3.16.3",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-force-graph": "^1.45.0",
    "recharts": "^2.15.3",
    "zod": "^3",
    "zustand": "^5.0.4",
    "react-force-graph-2d": "^1.27.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.79.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-modal": "^3.16.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "axe-core": "^4.10.3",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.8.6",
    "next-themes": "^0.4.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "theme-custom-properties": "^1.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
